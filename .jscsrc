{
    "preset": "yandex",
    "disallowMultipleLineStrings": null,
    "requireTrailingComma": {
        "ignoreSingleLine": true
    },
    "requireSpacesInAnonymousFunctionExpression": {
        "beforeOpeningRoundBrace": true,
        "beforeOpeningCurlyBrace": true,
        "allExcept": ["shorthand"]
    },
    "esnext": true
}
