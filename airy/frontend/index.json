{
    "app": {
        "es": "airy/frontend/app/**/!(*spec).js",
        "js": "airy/frontend/js/**/*.js",
        "jsmaps": "airy/frontend/js/**/*.js.map",
        "partials": "airy/frontend/app/**/*.html",
        "scss": "airy/frontend/app/**/*.scss",
        "css": "airy/frontend/css/**/*.css",
        "cssmaps": "airy/frontend/css/**/*.css.map",
        "fonts": "airy/frontend/fonts/*"
    },
    "specs": "airy/frontend/app/**/*.spec.js",
    "lib": {
        "js": [
            "bower_components/system.js/dist/system.js",
            "bower_components/system.js/dist/system-polyfills.js",
            "bower_components/jquery/dist/jquery.js",
            "bower_components/momentjs/moment.js",
            "bower_components/angular/angular.js",
            "bower_components/angular-sanitize/angular-sanitize.js",
            "bower_components/angular-linkify/angular-linkify.js",
            "bower_components/ng-dialog/js/ngDialog.js",
            "bower_components/angular-hotkeys/build/hotkeys.js",
            "bower_components/angular-elastic/elastic.js",
            "bower_components/angular-loading-bar/build/loading-bar.js",
            "bower_components/angular-ui-router/release/angular-ui-router.js",
            "bower_components/angular-breadcrumb/dist/angular-breadcrumb.js"
        ],
        "css": [
            "bower_components/pure/base.css",
            "bower_components/pure/forms.css",
            "bower_components/pure/buttons.css",
            "bower_components/pure/tables.css",
            "bower_components/ng-dialog/css/ngDialog.css",
            "bower_components/angular-loading-bar/build/loading-bar.css"
        ]
    }
}

